<?php
// Create a 3x3 matrix (multidimensional array)
$matrix = array(
    array(1, 2, 3),
    array(4, 5, 6),
    array(7, 8, 9)
);

// Access second row, third column
$value = $matrix[1][2]; // Row index starts at 0, so [1][2] = 6

echo "The value in the second row and third column is: $value";
?>
